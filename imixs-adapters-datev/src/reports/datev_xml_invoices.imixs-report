 AND _datev_client_id:["" TO *])    </value></item><item name="xsl"><value xsi:type="xs:string">&lt;?xml version="1.0" encoding="UTF-8" standalone="yes"?&gt;&lt;xsl:stylesheet	xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="2.0"&gt;	&lt;xsl:strip-space elements="*" /&gt;	&lt;xsl:output method="xml" indent="yes" encoding="UTF-8"		standalone="yes" /&gt;	&lt;xsl:template match="/"&gt;		&lt;xsl:variable name="date"			select="/data/document/item[@name='$modified']/value" /&gt;		&lt;xsl:variable name="subject"			select="/data/document/item[@name='_subject']/value" /&gt;		&lt;xsl:variable name="currency"			select="/data/document/item[@name='_currency']/value" /&gt;		&lt;xsl:variable name="invoiceid"			select="/data/document/item[@name='_invoicenumber']/value" /&gt;		&lt;xsl:variable name="gegenkonto"			select="/data/document/item[@name='_kreditor_konto']/value" /&gt;		&lt;LedgerImport			xmlns="http://xml.datev.de/bedi/tps/ledger/v040"			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"			xsi:schemaLocation="http://xml.datev.de/bedi/tps/ledger/v040 Belegverwaltung_online_ledger_import_v040.xsd"			version="4.0" generator_info="Imixs Office Workflow"			generating_system="Imixs Workflow"			xml_data="Kopie nur zur Verbuchung berechtigt nicht zum Vorsteuerabzug"&gt;			&lt;consolidate&gt;				&lt;xsl:attribute name="consolidatedAmount"&gt;&lt;xsl:value-of					select="/data/document/item[@name='_amount_brutto']/value" /&gt;&lt;/xsl:attribute&gt;				&lt;xsl:attribute name="consolidatedDate"&gt;&lt;xsl:value-of					select="format-dateTime($date, '[Y0001]-[M01]-[D01]')" /&gt;&lt;/xsl:attribute&gt;				&lt;xsl:attribute name="consolidatedInvoiceId"&gt;&lt;xsl:value-of					select="$invoiceid" /&gt;&lt;/xsl:attribute&gt;				&lt;xsl:attribute name="consolidatedCurrencyCode"&gt;&lt;xsl:value-of					select="$currency" /&gt;&lt;/xsl:attribute&gt;				&lt;xsl:for-each					select="/data/document/item[@name='_childitems']/value"&gt;					&lt;xsl:variable name="betrag"						select="re