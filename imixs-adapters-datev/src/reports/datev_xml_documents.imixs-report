    <item name="xsl">
        <value xsi:type="xs:string">&lt;?xml version="1.0" encoding="UTF-8"
            standalone="yes"?&gt;&lt;xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
            version="2.0"&gt; &lt;xsl:strip-space elements="*" /&gt; &lt;xsl:output method="xml"
            indent="yes" encoding="UTF-8" standalone="yes" /&gt; &lt;xsl:template match="/"&gt;
            &lt;xsl:variable name="now" select="current-dateTime()" /&gt; &lt;archive
            xmlns="http://xml.datev.de/bedi/tps/document/v04.0"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://xml.datev.de/bedi/tps/document/v04.0 Document_v040.xsd"
            version="4.0" generatingSystem="DATEV-Musterdaten"&gt; &lt;header&gt; &lt;date&gt;
            &lt;xsl:value-of select="format-dateTime($now, '[Y0001]-[M01]-[D01]T[H01]:[m01]:[s01]')"
            /&gt; &lt;/date&gt; &lt;description&gt;Belegsatzdaten
            Eingangsrechnung&lt;/description&gt; &lt;clientName&gt;Imixs Office
            Workflow&lt;/clientName&gt; &lt;/header&gt; &lt;content&gt; &lt;xsl:for-each
            select="/data/document"&gt; &lt;xsl:variable name="filename"
            select="./item[@name='$file']/value/item/@name" /&gt; &lt;xsl:variable name="date"
            select="./item[@name='$modified']/value" /&gt; &lt;document&gt; &lt;extension&gt;
            &lt;xsl:attribute name="xsi:type"&gt;accountsPayableLedger&lt;/xsl:attribute&gt;
            &lt;xsl:attribute name="datafile"&gt; &lt;xsl:value-of
            select="./item[@name='$uniqueid']/value" /&gt;&lt;xsl:text&gt;.xml&lt;/xsl:text&gt;
            &lt;/xsl:attribute&gt; &lt;property&gt; &lt;xsl:attribute
            name="value"&gt;&lt;xsl:value-of select="format-dateTime($date,
            '[Y0001]-[M01]')"/&gt;&lt;/xsl:attribute&gt; &lt;xsl:attribute
            name="key"&gt;1&lt;/xsl:attribute&gt; &lt;/property&gt; &lt;property&gt;
            &lt;xsl:attribute name="value"&gt;Eingangsrechnungen&lt;/xsl:attribute&gt;
            &lt;xsl:attribute name="key"&gt;3&lt;/xsl:attribute&gt; &lt;/property&gt;
            &lt;/extension&gt; &lt;extension&gt; &lt;xsl:attribute
            name="xsi:type"&gt;File&lt;/xsl:attribute&gt; &lt;xsl:attribute
            name="name"&gt;&lt;xsl:value-of select="$filename" /&gt;&lt;/xsl:attribute&gt;
            &lt;/extension&gt; &lt;/document&gt; &lt;/xsl:for-each&gt; &lt;/content&gt;
            &lt;/archive&gt; &lt;/xsl:template&gt;&lt;/xsl:stylesheet&gt;</value>
